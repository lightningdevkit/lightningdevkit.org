// https://google-webfonts-helper.herokuapp.com/fonts/manrope?subsets=latin
@font-face {
  font-family: 'Manrope';
  font-style: normal;
  font-weight: 400;
  src: url('/fonts/manrope-v4-latin-regular.woff2') format('woff2'),
       url('/fonts/manrope-v4-latin-regular.woff') format('woff');
}

/* manrope-600 - latin */
@font-face {
  font-family: 'Manrope';
  font-style: normal;
  font-weight: 600;
  src: url('/fonts/manrope-v4-latin-600.woff2') format('woff2'),
       url('/fonts/manrope-v4-latin-600.woff') format('woff');
}

// https://google-webfonts-helper.herokuapp.com/fonts/ibm-plex-mono?subsets=latin
/* ibm-plex-mono-regular - latin */
@font-face {
  font-family: 'IBM Plex Mono';
  font-style: normal;
  font-weight: 400;
  src: url('/fonts/ibm-plex-mono-v6-latin-regular.woff2') format('woff2'),
       url('/fonts/ibm-plex-mono-v6-latin-regular.woff') format('woff');
}
